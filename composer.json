{
    "name": "scybulski/laravel-validator-color",
    "description": "Validate that a certain value is a valid CSS color string in Laravel",
    "keywords": [
        "laravel",
        "validator",
        "color",
        "tylercd100",
        "scybulski"
    ],
    "homepage": "https://github.com/scybulski/laravel-validator-color",
    "license": "MIT",
    "authors": [
        {
            "name": "Szymon Cybulski",
            "email": "szymon9@windowslive.com"
        },
        {
            "name": "Tyler Arbon",
            "email": "tylercd100@gmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "scybulski\\Validator\\Color\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "scybulski\\Validator\\Color\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "illuminate/validation": ">=5.0 <7.0",
        "illuminate/support": ">=5.0 <7.0",
        "php": ">=7.0 <7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0|^6.0|^7.0|^8.0|^9.0",
        "orchestra/testbench": "^4.6"
    }
}
